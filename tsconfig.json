{
  "compilerOptions": {
    /* Language and Environment */
    "target": "es2016",
    "jsx": "react-jsx",
    /* Modules */
    "module": "commonjs",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "skipLibCheck": true
  }
}
